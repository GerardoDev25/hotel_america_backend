{
  "plugins": ["jsdoc-http-plugin"],
  "source": {
    "include": ["src"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules|docs)"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "recurse": true,
    "destination": "./doc",
    "template": "node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "theme": "dark"
    },
    "tutorials": "./tutorials",
    "readme": "README.md"
  }
}
